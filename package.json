{
  "name": "aws-devops-toolkit",
  "version": "0.0.1",
  "description": "Tool to consolidated various AWS DevOps scripts under one service",
  "main": "dist/bundle.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.ts",
    "type-check": "tsc --noEmit",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:dev": "webpack --config webpack/webpack.dev.js",
    "start": "yarn build:dev && node dist/bundle.js install-dependencies",
    "start:ts": "yarn build:dev && node dist/bundle.js ./temp-ts/",
    "start:js": "yarn build:dev && node dist/bundle.js ./temp-js/"
  },
  "bin": {
    "aws-devops-toolkit": "./dist/index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "peerDependencies": {
    "aws-sdk": "^2.683.0"
  },
  "files": [
    "dist"
  ],
  "author": "Christopher Caldwell",
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "eslint": "^7.2.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
